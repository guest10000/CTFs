import sys

count = 0

with open('measurements_day1', 'r') as f:
    lines = []
    for line in f:
        lines.append(line.strip())

print(lines)
print(len(lines))


for i in range(0,len(lines)):
    if int(lines[i]) > int(lines[i - 1]):
        count += 1
    else:
        continue
        
print(count) 
        
